; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "Node2D",
"class": "Bullet",
"language": "GDScript",
"path": "res://scenes/Bullet.gd"
}, {
"base": "Node2D",
"class": "Cat",
"language": "GDScript",
"path": "res://scenes/Cat.gd"
}, {
"base": "PathFollow2D",
"class": "Mouse",
"language": "GDScript",
"path": "res://scenes/Mouse.gd"
} ]
_global_script_class_icons={
"Bullet": "res://assets/ball.png",
"Cat": "res://assets/cat01.png",
"Mouse": "res://assets/mouse01.png"
}

[application]

config/name="Cats"
run/main_scene="res://scenes/Map.tscn"
config/icon="res://icon.png"

[display]

window/size/width=1536
window/size/height=768
window/size/test_width=800
window/size/test_height=400
window/stretch/mode="2d"
window/stretch/aspect="expand"

[gui]

common/drop_mouse_on_gui_input_disabled=true

[physics]

common/enable_pause_aware_picking=true

[rendering]

quality/driver/driver_name="GLES2"
vram_compression/import_etc=true
vram_compression/import_etc2=false
environment/default_environment="res://default_env.tres"
